{
  "name": "linguist-languages",
  "version": "9.0.0",
  "type": "module",
  "types": "lib/index.d.ts",
  "description": "Linguist languages data",
  "keywords": [
    "data",
    "json",
    "language",
    "linguist"
  ],
  "exports": {
    ".": "./lib/index.js",
    "./data/*": "./data/*.js"
  },
  "repository": "https://github.com/ikatyang-collab/linguist-languages",
  "homepage": "https://github.com/ikatyang-collab/linguist-languages#readme",
  "author": {
    "name": "Ika",
    "email": "ikatyang@gmail.com",
    "url": "https://github.com/ikatyang"
  },
  "license": "MIT",
  "scripts": {
    "prepack": "pnpm run build",
    "lint": "prettier --check .",
    "fix": "prettier --write .",
    "check": "tsc",
    "test": "vitest",
    "build": "node ./scripts/build.js"
  },
  "devDependencies": {
    "@types/node": "24.3.1",
    "@vitest/coverage-v8": "3.2.4",
    "camelcase": "8.0.0",
    "jest-snapshot-serializer-raw": "2.0.0",
    "node-style-text": "1.0.0",
    "outdent": "0.8.0",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "vite": "7.1.5",
    "vitest": "3.2.4",
    "yaml": "2.8.1"
  },
  "files": [
    "/lib/**/*",
    "/data/**/*",
    "/ThirdPartyNoticeText.txt"
  ],
  "packageManager": "pnpm@10.6.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "sideEffects": false
}
